<h3>{{ title }}</h3>

<table class="plot">
<th colspan="{{ cols }}">MOC; Meridional Heat & Freshwater Transports</th>
{% for t in plot_table %}
    <tr>
    {% for i in t %}
        {% if 'Error' in i %}
           <td>{{ i }}</td>
        {% else %}
           <td><a href="{{ i }}.gif">{{ i }}</td>
        {% endif %}
    {% endfor %}
    </tr>
{% endfor %}
</table>

<hr>
