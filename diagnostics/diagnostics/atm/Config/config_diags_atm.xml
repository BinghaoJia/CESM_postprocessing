<?xml version="1.0"?>

<!-- =========================================================================== -->
<!-- Atmoshperic Model Working Group Diagnostics XML                             -->
<!-- =========================================================================== -->

<config_definition version="1.0">

<groups>
  <!-- =========================================================================== -->
  <!-- atm_settings - general settings to be applied to all diagnostics routines   -->
  <!-- NOTE: case specific environment variables from env_*.xml will be            -->
  <!-- gathered at runtime by the atm_diags_generator.py python script.            -->
  <!-- They do not need to be specified in this XML file.                          -->
  <!-- The full $PATH variable will be set in the atm_diags_generator.py file      -->
  <!-- using all the path settings defined here.                                   -->
  <!-- =========================================================================== -->

  <group name="global">
  <order>0</order>
  <comment>Environmental variables that control which AMWG diagnostics to produce</comment>

  <entry id="ATMDIAG_DIAG_VERSION"
         type="integer"
         valid_values=""
         value="150626"
         group="diags_atm"
         desc="Diagnostic package version"
         ></entry>

  <entry id="ATMDIAG_MODEL_VS_OBS"
         type="logical"
         valid_values="True/False"
         value="True"
         group="diags_atm"
         desc="Controls whether to generate model vs. observations diagnostics"
         ></entry>

  <entry id="ATMDIAG_MODEL_VS_MODEL"
         type="logical"
         valid_values="True/False"
         value="False"
         group="diags_atm"
         desc="Controls whether to generate model vs. model diagnostics"
         ></entry>

  <entry id="ATMDIAG_CLEANUP_FILES"
         type="logical"
         valid_values="True/False"
         value="False"
         group="diags_atm"
         desc="Remove any existing diagnostic files."
         ></entry>
  </group>

<!-- ============================================================================================== -->
<!-- Set test datset                                                                                -->
<!-- ============================================================================================== -->

  <group name="test_params">
  <order>1</order>
  <comment>Set test datset</comment>

  <entry id="ATMDIAG_test_casename"
         type="char"
         valid_values=""
         value="$CASE"
         group="diags_atm"
         desc="Identifying test casename"
         ></entry>

  <entry id="ATMDIAG_test_modelstream"
         type="char"
         valid_values="cam.h0,cam2.h0"
         value="cam.h0"
         group="diags_atm"
         desc="Identifying test model stream"
         ></entry>

  <entry id="ATMDIAG_TEST_TIMESERIES"
         type="logical"
         valid_values="True/False"
         value="False"
         group="diags_atm"
         desc="Use tseries files to compute test climos."
         ></entry>

  <entry id="ATMDIAG_test_path_history"
         type="char"
         valid_values=""
         value="$DOUT_S_ROOT"
         group="diags_atm"
         desc="Path to test case"
         ></entry>

  <entry id="ATMDIAG_test_path_climo"
         type="char"
         valid_values=""
         value="$DOUT_S_ROOT/atm/proc/climo/$ATMDIAG_test_casename/"
         group="diags_atm"
         desc="Path to generated test climos"
         ></entry>

  <entry id="ATMDIAG_test_path_diag"
         type="char"
         valid_values=""
         value="$DOUT_S_ROOT/atm/proc/diag/$ATMDIAG_test_casename/"
         group="diags_atm"
         desc="Path to generated diags."
         ></entry>
  </group>

<!-- ============================================================================================== -->
<!-- Set cntl datset                                                                                -->
<!-- ============================================================================================== -->
  <group name="cntl_params">
  <order>2</order>
  <comment>Set control datset</comment>

  <entry id="ATMDIAG_cntl_casename"
         type="char"
         valid_values=""
         value=""
         group="diags_atm"
         desc="Identifying cntl casename"
         ></entry>

  <entry id="ATMDIAG_cntl_modelstream"
         type="char"
         valid_values="cam.h0,cam2.h0"
         value="cam.h0"
         group="diags_atm"
         desc="Identifying control model stream"
         ></entry>

  <entry id="ATMDIAG_CNTL_TIMESERIES"
         type="logical"
         valid_values="True/False"
         value="False"
         group="diags_atm"
         desc="Use tseries files to compute cntl climos."
         ></entry>

  <entry id="ATMDIAG_cntl_path_history"
         type="char"
         valid_values=""
         value=""
         group="diags_atm"
         desc="Path to cntl case"
         ></entry>

  <entry id="ATMDIAG_cntl_path_climo"
         type="char"
         valid_values=""
         value="$DOUT_S_ROOT/atm/proc/climo/$ATMDIAG_cntl_casename/"
         group="diags_atm"
         desc="Path to generated test climos"
         ></entry>

  <entry id="ATMDIAG_test_compute_climo"
         type="logical"
         valid_values="True/False"
         value="True"
         group="diags_atm"
         desc="Compute climatologies for the test case."
         ></entry>

  <entry id="ATMDIAG_cntl_compute_climo"
         type="logical"
         valid_values="True/False"
         value="False"
         group="diags_atm"
         desc="Compute climatologies for the cntl case."
         ></entry>
  </group>

<!-- ============================================================================================== -->
<!-- Set date ranges                                                                                -->
<!-- ============================================================================================== -->

  <group name="date_params">
  <order>3</order>
  <comment>Set date ranges</comment>

  <entry id="ATMDIAG_test_first_yr"
         type="integer"
         valid_values=""
         value=""
         group="diags_atm"
         desc="Start year for test case for diagnostics."
         ></entry>

  <entry id="ATMDIAG_test_nyrs"
         type="integer"
         valid_values=""
         value=""
         group="diags_atm"
         desc="The number of years of test data to be used for the diagnostics."
         ></entry>

  <entry id="ATMDIAG_cntl_first_yr"
         type="integer"
         valid_values=""
         value=""
         group="diags_atm"
         desc="Start year for cntl case for diagnostics."
         ></entry>

  <entry id="ATMDIAG_cntl_nyrs"
         type="integer"
         valid_values=""
         value=""
         group="diags_atm"
         desc="The number of years of cntl data to be used for the diagnostics."
         ></entry>

  </group>

<!-- ============================================================================================== -->
<!-- Input data paths                                                                               -->
<!-- ============================================================================================== -->

  <group name="input_data_paths">
  <order>4</order>
  <comment>Set input data paths</comment>

  <entry id="ATMDIAG_OBS_DATA"
         type="char"
         valid_values="$DIAGOBSROOT/amwg/obs_data"
         value=""
         group="diags_atm"
         desc="Local machine fully qualified directory path to AMWG observational data."
         ></entry>

  <entry id="ATMDIAG_CAM35_DATA"
         type="char"
         valid_values="$DIAGOBSROOT/amwg/cam35_data"
         value=""
         group="diags_atm"
         desc="Local machine fully qualified directory path to AMWG comparison data."
         ></entry>

  <entry id="ATMDIAG_MAP_DATA"
         type="char"
         valid_values="$DIAGOBSROOT/amwg/map_files"
         value=""
         group="diags_atm"
         desc="Local machine fully qualified directory path to AMWG mapping data."
         ></entry>

  </group>

<!-- ============================================================================================== -->
<!-- Set climo settings                                                                             -->
<!-- ============================================================================================== -->

  <group name="climo_settings">
  <order>5</order>
  <comment>Set climatology specific settings</comment>

  <entry id="ATMDIAG_strip_off_vars"
         type="logical"
         valid_values="True/False"
         value="True"
         group="diags_atm"
         desc="Strip off unused variables in the climo files."
         ></entry>

  <entry id="ATMDIAG_weight_months"
         type="logical"
         valid_values="True/False"
         value="True"
         group="diags_atm"
         desc="Weight the months by their number of days."
         ></entry>

  </group>

