<h3>{{ title }}</h3>

{% for plot_table in plot_tables %} 
   <table class="plot">
   <th colspan="{{ cols }}"></th>
   {% for t in plot_table %}
       <tr>  
           {% for i in t %}
       	    {% if 'Error' in i %}
            <td>{{ i }}</td>
	            {% elif i == '' %}
           <td></td>
        {% else %}
           <td><a href="{{ i }}.{{ imgFormat }}">{{ i }}</td>
        {% endif %}
    {% endfor %}
    </tr>
{% endfor %}
</table>
{% endfor %}

