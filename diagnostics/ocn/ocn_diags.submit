#! /bin/csh -f

source ./Tools/ccsm_getenv || exit -2

./check_case || echo "check_case failed" && exit -2
set sdate = `date +"%Y-%m-%d %H:%M:%S"`

cat > templar <<EOF
  ${BATCHSUBMIT} ./Diagnostics/ocn/ocn_diags.run
EOF
source templar
if ($status != 0) then
  echo "ERROR: ocn_diags.submit problem sourcing templar " 
endif
rm templar
echo "ocn_diags.run submitted $sdate" >>& ./ocnDiagsStatus


